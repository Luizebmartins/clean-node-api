{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },"plugins": [
    ],
    "rules": {
        "semi": ["error", "never"],
        "max-classes-per-file": [
            "error",
            { "ignoreExpressions": true, "max": 100 }
        ]
    }
}
  
